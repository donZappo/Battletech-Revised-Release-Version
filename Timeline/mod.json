{
    "Name": "Timeline",
    "DLL": "Timeline.dll",

    "Version": "1.3.0",
    "Description": "Grounds BattleTech in a particular time period in universe.",
    "Author": "mpstark",
    "Website": "www.github.com/BattletechModders/Timeline",

    "ConflictsWith": [ "AddYearToTimeline" ],
    "CustomResourceTypes": [ "ForcedTimelineEvent", "ItemCollectionRequirement" ],

    "Settings": {
        "DateFormatString": "{0:yyyy MMMM d}{1}"
    },

    "Manifest": [
        { "Type": "SimGameEventDef", "Path": "Events" },
        { "Type": "ForcedTimelineEvent", "Path": "ForcedEvents" },
        { "Type": "ItemCollectionRequirement", "Path": "ItemCollectionRequirements" }
    ]
}
